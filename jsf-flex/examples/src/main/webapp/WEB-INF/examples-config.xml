<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config>
  <!-- Below application config is for Facelet support 
  <application>
    <locale-config>
      <default-locale>en</default-locale>
    </locale-config>
	<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  </application>
  -->
  
  <!-- navigational rules for Facelet flexSimple -->
  <navigation-rule>
    <from-view-id>/flexSimple/flexSimple.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/flexSimple/flexSimple.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for Facelet flexSimple end -->
  
  <!-- navigational rules for Facelet flexOverallExample -->
  <navigation-rule>
    <from-view-id>/overallExample/flexOverallExample.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/overallExample/flexOverallExampleSuccess.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for Facelet flexOverallExample end -->
  
  <!-- navigational rules for Facelet flexOverallExampleSuccess -->
  <navigation-rule>
    <from-view-id>/overallExample/flexOverallExampleSuccess.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>back</from-outcome>
      <to-view-id>/overallExample/flexOverallExample.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for Facelet flexOverallExampleSuccess end -->
  
  <!-- navigational rules for Facelet flexMultipleRequestDataGridExample -->
  <navigation-rule>
    <from-view-id>/multipleRequestDataGridExample/flexMultipleRequestDataGridExample.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/multipleRequestDataGridExample/flexMultipleRequestDataGridExample.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for Facelet flexMultipleRequestDataGridExample end -->
  
  
  
  
  
  <!-- navigational rules for flexSimple -->
  <navigation-rule>
    <from-view-id>/flexSimple/flexSimple.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/flexSimple/flexSimple.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for flexSimple end -->
  
  <!-- navigational rules for flexOverallExample -->
  <navigation-rule>
    <from-view-id>/overallExample/flexOverallExample.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/overallExample/flexOverallExampleSuccess.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for flexOverallExample end -->
  
  <!-- navigational rules for flexOverallExampleSuccess -->
  <navigation-rule>
    <from-view-id>/overallExample/flexOverallExampleSuccess.jsp</from-view-id>
    <navigation-case>
      <from-outcome>back</from-outcome>
      <to-view-id>/overallExample/flexOverallExample.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for flexOverallExampleSuccess end -->
  
  <!-- navigational rules for flexMultipleRequestDataGridExample -->
  <navigation-rule>
    <from-view-id>/multipleRequestDataGridExample/flexMultipleRequestDataGridExample.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/multipleRequestDataGridExample/flexMultipleRequestDataGridExample.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <!-- navigational rules for flexMultipleRequestDataGridExample end -->
    
</faces-config>
